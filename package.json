{
  "name": "@kreisler/dirname-for-module",
  "version": "1.0.7",
  "description": "dirname for module (import/export)",
  "homepage": "https://github.com/itskreisler/dirname-for-module#readme",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "jsdelivr": "dist/index.iife.js",
  "unpkg": "dist/index.iife.js",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "dirname",
    "fileUrlPath",
    "__dirname"
  ],
  "scripts": {
    "prepare": "pnpm run build && pnpm run docs",
    "build": "rogo",
    "watch": "rogo -w",
    "docs": "typedoc --out docs src/index.ts",
    "server": "pnpx servor docs index.html 4321",
    "me:publish": "npm publish --access public"
  },
  "author": "kreisler <tempkreisler@outlook.com> (https://github.com/itskreisler)",
  "bugs": {
    "url": "https://github.com/itskreisler/dirname-for-module/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itskreisler/dirname-for-module.git"
  },
  "devDependencies": {
    "rogo": "^3.1.4",
    "typedoc": "^0.22.13",
    "typescript": "^4"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "path": "^0.12.7",
    "url": "^0.11.3"
  },
  "license": "MIT"
}